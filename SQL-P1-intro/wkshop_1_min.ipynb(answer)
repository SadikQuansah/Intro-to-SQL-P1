    "#2.2.5\n",
    "\n",
    "import sqlite3\n",
    "\n",
    "# Create a connection to the SQLite database\n",
    "conn = sqlite3.connect('song.sqlite')\n",
    "\n",
    "\n",
    "\n",
    "# Update the values in the new column with the duration in m:s format\n",
    "cursor.execute(\"UPDATE song_data SET duration_minutes_seconds = CAST(song_duration_ms / 60000 AS TEXT) || ':' || CAST((song_duration_ms % 60000) / 1000 AS TEXT)\")\n",
    "\n",
    "# Commit the changes to the database\n",
    "conn.commit()\n",
    "\n",
