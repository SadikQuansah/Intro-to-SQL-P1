    "#2.2.1\n",
    "\n",
    "import sqlite3\n",
    "\n",
    "import sqlite3\n",
    "\n",
    "# Connect to the SQLite database\n",
    "conn = sqlite3.connect('song.sqlite')\n",
    "cursor = conn.cursor()\n",
    "\n",
    "# Specify your favorite song\n",
    "favorite_song = \"crazy\"\n",
    "\n",
    "# Query to check if the favorite song is in the dataset\n",
    "query = f\"SELECT COUNT(*) FROM song_data WHERE song_name = '{favorite_song}'\"\n",
    "\n",
    "# Execute the query\n",
    "cursor.execute(query)\n",
    "\n",
    "# Fetch the result\n",
    "result = cursor.fetchone()[0]\n",
    "\n",
    "\n",
    "\n",
    "result\n",
    "\n"
   ]
