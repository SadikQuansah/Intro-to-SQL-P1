   "source": [
    "#1.5\n",
    "\n",
    "import sqlite3\n",
    "\n",
    "# Connect to the SQLite database\n",
    "conn = sqlite3.connect('../data/mtcars.sqlite')\n",
    "cursor = conn.cursor()\n",
    "\n",
    "# Count the number of vehicles under 45 years\n",
    "query_under_45 = \"SELECT COUNT(*) FROM results WHERE vehicle_age < 45\"\n",
    "cursor.execute(query_under_45)\n",
    "count_under_45 = cursor.fetchone()[0]\n",
    "\n",
    "# Count the number of vehicles 45 years and over\n",
    "query_45_and_over = \"SELECT COUNT(*) FROM results WHERE vehicle_age >= 45\"\n",
    "cursor.execute(query_45_and_over)\n",
    "count_45_and_over = cursor.fetchone()[0]\n",
    "\n",
    "# Print the results\n",
    "print(\"Number of vehicles under 45 years:\", count_under_45)\n",
    "print(\"Number of vehicles 45 years and over:\", count_45_and_over)\n",
    "\n",
    "# Close the database connection\n",
    "conn.close()\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
