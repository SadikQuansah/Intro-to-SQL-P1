   "source": [
    "# 1.4\n",
    "\n",
    "import pandas as pd\n",
    "import sqlite3\n",
    "\n",
    "\n",
    "conn = sqlite3.connect('../data/mtcars.sqlite')\n",
    "cursor = conn.cursor()\n",
    "\n",
    "query = \"\"\"SELECT age_label, COUNT(*) as count\n",
    "           FROM results\n",
    "           WHERE vehicle_age < 45\n",
    "           GROUP BY age_label\"\"\"\n",
    "select_query = \"SELECT * FROM new_table\"\n",
    "cursor.execute(query)\n",
    "results = cursor.fetchall()\n",
    "\n",
    "for row in results:\n",
    "    print(row)\n",
    "\n",
    "\n",
    "conn.close()\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
